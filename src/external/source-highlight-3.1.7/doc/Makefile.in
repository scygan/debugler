@MAINTAINER_MODE_TRUE@@NO_HELP2MAN_FALSE@source-highlight.1: $(top_srcdir)/src/cmdline.ggo $(top_builddir)/config.h
@MAINTAINER_MODE_TRUE@@NO_HELP2MAN_FALSE@	$(HELP2MAN) -n "convert source code to syntax highlighted document" $(SRCHILITEEXE) > $@

