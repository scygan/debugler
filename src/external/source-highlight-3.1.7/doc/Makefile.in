$(srcdir)/stamp-1: source-highlight-lib.texinfo $(top_srcdir)/configure
	@(dir=.; test -f ./source-highlight-lib.texinfo || dir=$(srcdir); \
	set `$(SHELL) $(top_srcdir)/build-aux/mdate-sh $$dir/source-highlight-lib.texinfo`; \
	echo "@set UPDATED $$1 $$2 $$3"; \
	echo "@set UPDATED-MONTH $$2 $$3"; \
	echo "@set EDITION $(VERSION)"; \
